<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @list.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @list.description %>
</p>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Location</th>
  </tr>

  <% @list.places.build.each do |place| %>
